<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="NumberedButtons" >
        <Setter Property="Button.Background" Value="#FFDCDBDB"/>
        <Setter Property="Button.FontSize" Value="20"/>
        <Setter Property="Button.Margin" Value="1"/>
        <Setter Property="Button.Padding" Value="1"/>
        <!--<Setter Property="Corn" Value="1"/>-->
    </Style>
    <Style x:Key="ComputingButtons">
        <Setter Property="Button.Background" Value="Gray"/>
        <Setter Property="Button.FontSize" Value="20"/>
        <Setter Property="Button.Margin" Value="1"/>
        <Setter Property="Button.Padding" Value="1"/>
    </Style>


</ResourceDictionary>